<%@page contentType="text/html;charset=UTF-8"%><%@page pageEncoding="UTF-8"%><%@ page session="false"%><%@ taglib prefix="form" uri="http://www.springframework.org/tags/form"%><%@ include file="/WEB-INF/jsp/common/include.jsp"%><script language="JavaScript">	function ValidateChanges() {		var image = document.getElementById("image").value;		if (image != '') {			var checkimg = image.toLowerCase();			if (!checkimg.match(/(\.jpg|\.png|\.JPG|\.PNG|\.jpeg|\.JPEG)$/)) {				alert("Please enter  Image  File Extensions .jpg,.png,.jpeg");				document.getElementById("image").focus();				return false;			}		}		return true;	}</script><div class="tabContainer_item">	<div class="infoLayer_toggler" style="padding-top: 15px;padding-bottom: 10px;">		<div style="display: inline;"><a href="#" data-content="customer_sfr"			class="togglerMultipack toggler-a closeState">			<img src="<c:url value='/img/database-icon.png'/>" width="16" height="23" border="0" alt="Editer les propriétés" />			Upload now!</a>			<c:if test="${controller eq 'albumsController'}">			<%-- 				<a href="#" onclick="javascript: validateChanges(${currentAlbum.id})" class="togglerMultipack toggler-a closeState"> --%><%-- 				<img src="<c:url value='/img/save-icon.png'/>" width="16" height="23" border="0" alt="Editer les propriétés" /> --%><!-- 				Save Album</a> -->				</c:if>		</div>	</div>	<div id="customer_sfr" class="toggleHide">		<input id="fileupload" type="file" name="files[]"			data-url="<c:url value='/${controller}/upload.html' />" multiple><!-- 		<div id="dropzone" class="fade well">Drop files here</div> -->		<div id="progress" class="progress">			<div class="bar" style="width: 0%;"></div>		</div>	</div></div><table id="uploaded-files" class="myDataTable display"	style="width: 650px; padding: 20px">	<thead>		<tr>			<th align="center"> Image</th>			<th class="first">File Name</th>			<th>File Size</th>			<th>File Dimention</th>			<th class="last">Actions</th>		</tr>	</thead>	<tbody>	</tbody>	<tfoot>		<tr class="footer noBorder">			<td id="lastBottom" class="lastBottom" colspan="5" data-active-page='${pagination.activePageNumber}'>				<%@ include file="/WEB-INF/jsp/page/images/pagination.jsp" %>			</td>		</tr>		<tr>			<td colspan="5">				 <select id="massiveAction" class="selectionactions">					<option value="NONE" selected="selected">CHOISIR UNE ACTION</option>					<option value="REMOVE_SELECTED" onclick="javascript:massiveAction();">Supprimer la selection</option>					<option value="REMOVE_ALL" onclick="javascript:showPopupWarning('popupLayerSupprimerTout');">Supprimer tout</option>					<option value="TOOGLE_SELECT_ALL" onclick="javascript:massiveAction();">Sélectionner/Déselectionner tout</option>					<option value="CREATE_ALBUM" id="createAlbumWithModal" onclick="javascript:massiveAction();">Créer un album</option>				</select>			</td>		</tr>	</tfoot></table><%@ include file="/WEB-INF/jsp/page/albums/body/createAlbum.jsp"%><c:set var="elementId" value="SupprimerTout" /><c:set var="confirmfunction" value="massiveAction();" /><c:set var="messageHeader" value="Suppression de toutes les photos de l'album ${currentAlbum}" /><c:set var="messageBody" value="Attention, cette suppression sera <b>irréversible</b><br/> <b>Toutes les pages</b> de l'album seront supprimés!!" /><%@ include file="/WEB-INF/jsp/common/popup.jsp"%>