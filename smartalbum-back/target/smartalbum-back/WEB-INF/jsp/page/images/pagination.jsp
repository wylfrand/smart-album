<div class="pagination">	<ul id="pagination-digg">	 <li class="previous-off"><a href="<c:url value='/${controller}/page/${pagination.previous}.html?view=${view}&albumId=${currentAlbum.id}'/>">« Précédent</a></li>	    <c:forEach var="position" begin="1" end="${pagination.nbPage}" step="1">		   <c:choose>		   		<c:when test="${pagination.activePageNumber eq position}">		   			<li class="active">${position}</li>		   		</c:when>		   		<c:otherwise>		   			<li><a href="<c:url value='/${controller}/page/${position}.html?view=${view}&albumId=${currentAlbum.id}'/>">${position}</a></li>		   		</c:otherwise>		   </c:choose>	    </c:forEach>	    <li class="next"><a href="<c:url value='/${controller}/page/${pagination.next}.html?view=${view}&albumId=${currentAlbum.id}'/>">Suivant »</a></li>	</ul></div>